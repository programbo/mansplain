{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "strict": true,
    "typeRoots": [
      "@types"
    ],
    "esModuleInterop": true,
    "allowJs": true,
    "skipLibCheck": false,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve"
  },
  "include": [
    "src"
  ]
}
